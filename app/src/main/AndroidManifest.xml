<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizyou">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".User.StudentActivity"></activity>
        <activity android:name=".User.TeacherActivity"></activity>
        <activity android:name=".User.ReportActivity"></activity>
        <activity android:name=".User.ViewGradedTestActivity"></activity>
        <activity android:name=".Test.TestActivity"></activity>
        <activity android:name=".Test.GradeTestActivity"></activity>
        <activity android:name=".Test.MakeTestActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>